{
    "name": "jquery-crate",
    "version": "0.0.10",
    "author": "Chat-Wane <grumpy.chat.wane@gmail.com>",
    "description": "Cratify tool that turns a division into a distributed and decentralized collaborative editor",
    "main": "./lib/cratify.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/haouarin/jquery-crate.git"
    },
    "keywords": [
        "Random Peer Sampling",
        "Adaptive",
        "browser-to-browser communication",
        "WebRTC"
    ],
    "dependencies": {
        "BigInt": "^5.4.5",
        "animals": "git://github.com/nhaouari/animals#master",
        "brace": "0.5.x",
        "clone": "^1.0.3",
        "debug": "^2.6.1",
        "jquery-qrcode": "git://github.com/lrsjng/jquery-qrcode.git#master",
        "lodash": "^4.17.4",
        "lodash.merge": "^4.6.1",
        "marked": "0.3.x",
        "simple-peer": "^8.1.1",
        "socket.io-client": "1.3.x",
        "sorted-cmp-array": "2.0.x",
        "string-hash": "1.1.x",
        "uglify-js": "^1.3.5",
        "unicast-definition": "0.1.x",
        "uuid": "^3.0.1",
        "shortid": "*"
    },
    "devDependencies": {
        "browserify": "6.2.x",
        "brfs": "1.4.x"
    },
    "browser": {
        "jquery-qrcode": "./node_modules/jquery-qrcode/dist/jquery-qrcode.js"
    },
    "browserify": {
        "transform": [
            "brfs"
        ]
    },
    "scripts": {
        "debug": "browserify lib/cratify.js --debug > build/jquery-crate.bundle.debug.js",
        "build": "browserify lib/cratify.js > build/jquery-crate.bundle.js",
        "watch": "watchify lib/cratify.js -o build/jquery-crate.bundle.js -d -v"
    },
    "license": "MIT"
}
